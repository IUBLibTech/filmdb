<h4 class="mt_25">Physical Objects With This Title</h4>
<% if @title.physical_objects.size == 0 %>
    none
<% else %>
<table class="_100">
  <tr>
    <th></th>
    <th>Barcode</th>
    <th>Medium</th>
    <th>Gauge</th>
    <th>Overall Condition</th>
    <th>Generation</th>
    <th>Reel Number</th>
    <th>Can Size</th>
  </tr>
  <% @title.physical_objects.each do |p| %>
  <tr>
    <td><%= check_box_tag "physical_object_#{p.id}", {class: "po_check_box"} %></td>
    <td><%= link_to "#{p.iu_barcode}", physical_object_path(p) %></td>
    <td><%= p.medium %></td>
    <td><%= p.gauge %></td>
    <td><%= p.condition_rating %></td>
    <td><%= p.humanize_boolean_fields(PhysicalObject::GENERATION_FIELDS) %></td>
    <td><%= p.reel_number %></td>
    <td><%= p.can_size %></td>
  </tr>
  <% end %>
</table>
<% end %>