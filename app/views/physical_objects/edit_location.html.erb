<%= form_for @physical_object, url: update_location_path do |f| %>
    <table>
      <tr>
        <th>IU Barcode</th>
        <td>
          <%= f.text_field :iu_barcode %>
        </td>
      </tr>
      <tr>
        <th>Location</th>
        <td>
          <%= f.select :location, options_for_select(@cv[:location]) %>
        </td>
      </tr>
      <tr>
        <td colspan="2">
          <%= f.submit 'Update Location' %>
        </td>
      </tr>
    </table>
<% end %>