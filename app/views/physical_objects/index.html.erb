<p id="notice"><%= notice %></p>

<h1>Listing Physical Objects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @physical_objects.each do |physical_object| %>
      <tr>
        <td><%= link_to 'Show', physical_object %></td>
        <td><%= link_to 'Edit', edit_physical_object_path(physical_object) %></td>
        <td><%= link_to 'Destroy', physical_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Physical object', new_physical_object_path %>
