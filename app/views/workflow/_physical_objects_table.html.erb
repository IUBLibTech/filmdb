<table id="physical_objects_table">
	<tr>
		<th>IU Barcode</th>
		<th>Title(s)</th>
    <th>Location</th>
	</tr>
<% @physical_objects.each do |p| %>
	<tr>
		<td><%=  p.iu_barcode %></td>
		<td><%= p.titles_text %></td>
    <td><%=  p.current_workflow_status.workflow_status_location %></td>
	</tr>
<% end %>
</table>
