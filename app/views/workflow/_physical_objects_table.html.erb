<table id="physical_objects_table">
	<tr>
		<th>IU Barcode</th>
		<th>Title(s)</th>
		<th>Action</th>
	</tr>
<% @physical_objects.each do |p| %>
	<tr>
		<td><%=  p.iu_barcode %></td>
		<td><%= p.titles_text %></td>
		<td><%= link_to "#{@action_text}", @url+"#{p.id}", remote: true, class: 'action_link' %></td>
	</tr>
<% end %>
</table>
